<div class="jumbotron home m-t-100 display-none" id="home1">
    {{#switch user.role.code}}
    {{#case "1" break=true}}
    <div class="container m-t-20">
        <div class="m-l-250">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
                <form id="uploadIdForm" class="login100-form validate-form">
                    <div class="wrap-input100 rs1 validate-input" data-validate="Identity type is required">
                        <select name="identityType" class="form-control input100" id="sel2">
                            <option value="Aadhar">Aadhar</option>
                            <option value="Pan Card">Pan Card</option>
                        </select>
                    </div>

                    <div class="wrap-input100 rs1 validate-input" data-validate="Mobile is required">
                        <label for="exampleFormControlFile1">Example file input</label>
                        <input type="file" class="form-control-file" id="exampleFormControlFile1">
                        <span class="focus-input100-1"></span>
                        <span class="focus-input100-2"></span>
                    </div>
                    <p class="text-green display-none" id="uploadIdSuccessMsg">Successfully Uploaded!!</p>
                    <p class="text-red display-none" id="uploadIdErrorMsg">Something went wrong, try again later.</p>

                    <div class="container-login100-form-btn m-t-20">
                        <button id="uploadIdFormSubmit" class="login100-form-btn">
                            Upload
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    {{/case}}{{#case "4" break=true}}
    <div>
        <form class="form-inline">
            <input class="form-control form-control-sm ml-3 w-75" id="identityVerificationSearch" type="search"
                placeholder="Search citizen..." aria-label="Search">
        </form>
    </div>
    <table id="identityVerificationTable" class="ui celled selectable table no-mt no-pa">
        <thead>
            <tr>
                <th>Name</th>
                <th>Id Type</th>
                <th>File</th>
                <th>Tx hash</th>
                <th>Data Hash</th>
                <th>Verification Status</th>
                {{!-- <th></th> --}}
                {{!-- <th>CreatedAt</th> --}}
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
    {{/case}}
    {{/switch}}
</div>

